{
    "name": "deniztezcan/laravel-responsecache-additions",
    "description": "Speed up a Laravel application by caching the entire response additions",
    "keywords": [
        "spatie",
        "laravel-responsecache",
        "laravel",
        "cache",
        "response",
        "performance"
    ],
    "homepage": "https://github.com/deniztezcan/laravel-responsecache-additions",
    "license": "MIT",
    "authors": [
        {
            "name": "Deniz Tezcan",
            "email": "deniz@nopulp.nl",
            "homepage": "https://nopulp.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "jenssegers/agent": "^2.6",
        "spatie/laravel-responsecache": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "DenizTezcan\\ResponseCache\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
